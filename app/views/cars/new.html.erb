<h1>List Your Car</h1>
<%= simple_form_for @car do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :rental_options %>
  <%= f.input :rent_price %>
  <%= f.input :features %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.submit "Submit Car", class: "btn btn-primary" %>
<% end %>
